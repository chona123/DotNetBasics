@model List<ListMVC.Models.Employee>
    


@{
        ViewBag.Title = "GetEmployeeIndex";
    }

    <h2>GetEmployeeIndex</h2>

   
<div>

    @foreach (var item in Model)
    {
        <div>
            @item.Id
        </div>
  
